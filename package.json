{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "16.15.0"
  },
  "main": "index.js",
  "scripts": {
    "build-tsc": "npx tsc",
    "client-install": "npm install --prefix client",
    "build": "concurrently \"cd client && npm run build\" \"npm build \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "cd ./client && npm start ",
    "server-prod": "npm run build-tsc && node build/index.js",
    "server": "npm run build-tsc && nodemon build/index.js",
    "start": "npm run build-tsc && node build/index.js",
    "start:dev": "concurrently --kill-others  \"npm run client\" \"npm run server\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
